    <property>
      <code>#template('colorThemeInit.vm')
#set ($settings = $xwiki.getDocument('CookieConsent.WebHome').getObject('CookieConsent.ConfigurationClass', 'lang', "$xcontext.locale"))
#if (!$settings)
  #set ($settings = $xwiki.getDocument('CookieConsent.WebHome').getObject('CookieConsent.ConfigurationClass'))
#end
#cookieConsent {
  position: fixed;
  #if($settings.getProperty('type').value == 'bar')
    $settings.getProperty('position').value: 0;
    left: 0;
    right: 0;
  #else
    $settings.getProperty('position').value: 10px;
    left: 50%;
    transform:translatex(-50%);
    max-width: 400px;
  #end
  background: $settings.getProperty('backgroundColor').value;
  color: $settings.getProperty('foregroundColor').value;
  text-align: center;
  line-height: 1.5;
  overflow: hidden; 
  opacity: $settings.getProperty('opacity').value;
  z-index: 9999;
  padding: 10px;
  -webkit-box-shadow: 0 0 5px black;
  -moz-box-shadow:    0 0 5px black;
  box-shadow:         0 0 5px black;
}

#cookieConsentHide, #cookieConsentConfig, #cookieConsentOK, #cookieConsentRejectAll {
  appearance: button;
  border: 2px solid @btn-primary-bg;
  transition: all 0.3s;
  &amp;:hover {
    transition: all 0.3s;
  }
}

#cookieConsentHide, #cookieConsentOK {
  background-color: @btn-primary-bg;
  color: @btn-primary-color;
  &amp;:hover {
    background-color: @btn-primary-color;
    color: @btn-primary-bg;
  }
}

#cookieConsentConfig, #cookieConsentRejectAll {
  background-color: @btn-primary-color;
  color: @btn-primary-bg;
  &amp;:hover {
    background-color: @btn-primary-bg;
    color: @btn-primary-color;
  }
}
#cookieConsent .button {
  #if($settings.getProperty('type').value == 'box')
    margin: 10px 5px 0 5px; 
  #else
    margin: 0 5px;
  #end
  min-width: 90px;
  padding: 6px;
}
#CookieSelectPane {
    display: inline-block;
    border: 1px solid #cccccc;
    padding: 6px 3px 5px 9px;
    text-align: left;
    vertical-align: top;
}
.CookieSelectButtonWrapper {
  #if($settings.getProperty('type').value == 'bar')
    display: inline-block;
  #end
    position: relative;
    margin-right: 14px;
    margin-left: 0;
    line-height: 16px;
}
#CookieSelectPane label {
  font-weight: normal;
  display: block;
  padding-left: 15px;
  text-indent: -15px;
}
#CookieSelectPane input {
  width: 13px;
  height: 13px;
  padding: 0;
  margin:0 5px;
  vertical-align: bottom;
  position: relative;
  top: -1px;
  *overflow: hidden;
}</code>
